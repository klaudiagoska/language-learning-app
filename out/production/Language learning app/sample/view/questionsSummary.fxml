<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="450.0" style="-fx-background-color: #b8abcb;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.QuestionsSummary">
   <children>
      <Label fx:id="question" alignment="CENTER" layoutX="142.0" layoutY="90.0" prefHeight="18.0" prefWidth="165.0" text="Question">
         <font>
            <Font name="SansSerif Regular" size="12.0" />
         </font></Label>
      <Label fx:id="userAnswer" alignment="CENTER" layoutX="89.0" layoutY="131.0" prefHeight="18.0" prefWidth="271.0" text="User asnwer">
         <font>
            <Font name="SansSerif Regular" size="12.0" />
         </font></Label>
      <Label fx:id="correctAnswer" alignment="CENTER" layoutX="89.0" layoutY="159.0" prefHeight="18.0" prefWidth="271.0" text="Correct answer">
         <font>
            <Font name="SansSerif Regular" size="12.0" />
         </font></Label>
      <Button layoutX="180.0" layoutY="197.0" mnemonicParsing="false" onAction="#nextQuestion" text="Next question">
         <font>
            <Font name="SansSerif Regular" size="12.0" />
         </font></Button>
      <Button layoutX="155.0" layoutY="247.0" mnemonicParsing="false" onAction="#goToMenu" text="Menu">
         <font>
            <Font name="SansSerif Regular" size="12.0" />
         </font></Button>
      <Label layoutX="146.0" layoutY="27.0" text="Check your answer">
         <font>
            <Font name="SansSerif Regular" size="19.0" />
         </font>
      </Label>
      <Button layoutX="245.0" layoutY="247.0" mnemonicParsing="false" onAction="#end" prefHeight="22.0" prefWidth="46.0" text="End">
         <font>
            <Font name="SansSerif Regular" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
