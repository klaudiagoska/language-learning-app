<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.InputQuestion">
   <children>
      <Label fx:id="question" layoutX="282.0" layoutY="75.0" prefHeight="18.0" prefWidth="36.0" text="Label" />
      <TextField fx:id="userAnswer" layoutX="225.0" layoutY="135.0" />
      <Button fx:id="answer" layoutX="199.0" layoutY="213.0" mnemonicParsing="false" onAction="#answer" text="Answer" />
      <Button fx:id="nextQuestion" layoutX="328.0" layoutY="213.0" mnemonicParsing="false" onAction="#nextQuestion" text="Next question" />
      <Label fx:id="alert" layoutX="286.0" layoutY="291.0" text="Label" />
   </children>
</AnchorPane>
